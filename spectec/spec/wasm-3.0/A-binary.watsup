;; Vector instructions

grammar Blaneidx : laneidx =
  | l:Bbyte => l

grammar Binstr/vector-memory : instr = ...
  | 0xFD 0:Bu32 (x,mo):Bmemop => VLOAD x mo
  | 0xFD 1:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 8 X 8 S) x mo
  | 0xFD 2:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 8 X 8 U) x mo
  | 0xFD 3:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 16 X 4 S) x mo
  | 0xFD 4:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 16 X 4 U) x mo
  | 0xFD 5:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 32 X 2 S) x mo
  | 0xFD 6:Bu32 (x,mo):Bmemop => VLOAD (SHAPE 32 X 2 U) x mo
  | 0xFD 7:Bu32 (x,mo):Bmemop => VLOAD (SPLAT 8) x mo
  | 0xFD 8:Bu32 (x,mo):Bmemop => VLOAD (SPLAT 16) x mo
  | 0xFD 9:Bu32 (x,mo):Bmemop => VLOAD (SPLAT 32) x mo
  | 0xFD 10:Bu32 (x,mo):Bmemop => VLOAD (SPLAT 64) x mo
  | 0xFD 92:Bu32 (x,mo):Bmemop => VLOAD (ZERO 32) x mo
  | 0xFD 92:Bu32 (x,mo):Bmemop => VLOAD (ZERO 64) x mo
  | 0xFD 11:Bu32 (x,mo):Bmemop => VSTORE x mo
  | 0xFD 84:Bu32 (x,mo):Bmemop l:Blaneidx => VLOAD_LANE 8 x mo l
  | 0xFD 85:Bu32 (x,mo):Bmemop l:Blaneidx => VLOAD_LANE 16 x mo l
  | 0xFD 86:Bu32 (x,mo):Bmemop l:Blaneidx => VLOAD_LANE 32 x mo l
  | 0xFD 87:Bu32 (x,mo):Bmemop l:Blaneidx => VLOAD_LANE 64 x mo l
  | 0xFD 88:Bu32 (x,mo):Bmemop l:Blaneidx => VSTORE_LANE 8 x mo l
  | 0xFD 89:Bu32 (x,mo):Bmemop l:Blaneidx => VSTORE_LANE 16 x mo l
  | 0xFD 90:Bu32 (x,mo):Bmemop l:Blaneidx => VSTORE_LANE 32 x mo l
  | 0xFD 91:Bu32 (x,mo):Bmemop l:Blaneidx => VSTORE_LANE 64 x mo l
  | ...

grammar Binstr/vector-const : instr = ...
  | 0xFD 12:Bu32 (b:Bbyte)^16 => VCONST V128 b' -- if $ibytes(128, b') = b
  | ...

grammar Binstr/vector-shuffle : instr = ...
  | 0xFD 13:Bu32 (l:Blaneidx)^16 => VSHUFFLE (I8 X 16) l
  | ...

